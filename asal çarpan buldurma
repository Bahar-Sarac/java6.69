
import java.util.Scanner;

public class asalcarpanbuldurma {

    public static void main(String[] args) {
        Scanner input = new Scanner (System.in);
        
        System.out.print("Pozitif tam sayÄ± giriniz: ");
        int n = input.nextInt();
        int b = 2;
        
        while(n>1){
            if(n % b == 0){
                n = n / b;
                System.out.println(b);
            }
            else if(n % b != 0){
                b += 1; 
            }
        }
    }
    
}
